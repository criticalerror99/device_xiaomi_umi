allow vendor_init system_data_file : dir { setattr }; 
allow vendor_init device : chr_file { getattr };
# To get/set property
set_prop(vendor_init, default_prop)
allow vendor_init {
    default_prop
}:file rw_file_perms;
allow vendor_init block_device : lnk_file { setattr }; 

typeattribute vendor_init data_between_core_and_vendor_violators;

allow vendor_init {
  bluetooth_data_file
  camera_data_file
  dhcp_data_file
  media_rw_data_file
  system_data_file
  tombstone_data_file
  wifi_data_file
  wpa_socket
}:dir { create search getattr open read search setattr ioctl write add_name remove_name rmdir relabelfrom };
